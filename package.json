{
  "name": "ts-node-skeleton",
  "version": "1.0.0",
  "description": "A skeleton for use typescript with NodeJs",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --coverage --collectCoverageFrom=src/**",
    "build": "rm -rf dist/* && tsc --sourceMap false",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "pretty-quick": "pretty-quick",
    "pretty-check": "pretty-quick --check",
    "precommit": "npm run lint && npm run pretty-quick --staged && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "your-repo-url"
  },
  "keywords": [
    "node",
    "ts",
    "skeleton",
    "nodejs",
    "typescript"
  ],
  "author": "vickodev",
  "license": "MIT",
  "homepage": "your-home-page",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^26.0.4",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.0.18",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-mock-extended": "^1.0.9",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.1.0",
    "moment": "^2.27.0",
    "typed-rest-client": "^1.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
